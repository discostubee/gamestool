
<project name="eclipse_project_name">
	<target name="postBuildLib">
		<echo>${eclipse_includes_dir}</echo>
		<exec executable="[ ! -d ${eclipse_includes_dir}] mkdir ${eclipse_includes_dir}">
		</exec>
		<exec executable="cp">
			<arg value="-f" />
			<arg value="${eclipse_target_path}" />
			<arg value="${eclipse_includes_dir}" />
		</exec>
	</target>
	<target name="postBuildBin">
		<echo>${eclipse_includes_dir}</echo>
		<exec executable="[ ! -d ${eclipse_includes_dir}] mkdir ${eclipse_includes_dir}">
		</exec>
		<exec executable="cp">
			<arg value="-f" />
			<arg value="${eclipse_target_path}" />
			<arg value="${eclipse_includes_dir}" />
		</exec>
	</target>
</project>